		<UPDATE_CODE>
		(L:152X_COM1VOL, bool) 1 &gt;=
		if{
			100 (&gt;K:COM1_VOLUME_SET)
			#COM_CIRCUIT# (&gt;K:ELECTRICAL_CIRCUIT_ON)
		}
		(L:152X_COM1VOL, Bool) 0 &lt;=
		if{
			0 (&gt;K:COM1_VOLUME_SET)
			#COM_CIRCUIT# (&gt;K:ELECTRICAL_CIRCUIT_OFF)
		}
		</UPDATE_CODE>	
	</UseTemplate>
</Template>	
