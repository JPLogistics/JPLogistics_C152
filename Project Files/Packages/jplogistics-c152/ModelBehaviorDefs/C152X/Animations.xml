<Template Name="Misc_Helpers">
	<Update Frequency="3">
		(L:JPL_WINDOW_PILOT, position 16k) (L:JPL_WINDOW_COPILOT, position 16k) - 1638.4 / (&gt;K:AILERON_TRIM_SET)
	</Update>


	<!--### DOOR CLOSE ###-->
	<Update Frequency="2">
		(A:AIRSPEED TRUE, knots) 30 &gt;=
		(L:JPL_DOOR_PILOT) 1 &gt;= and
		if{
			0 (&gt;L:JPL_DOOR_PILOT, bool)
		}	
	</Update>
	<Update Frequency="1.7">
		(A:AIRSPEED TRUE, knots) 30 &gt;=
		(L:JPL_DOOR_COPILOT) 1 &gt;= and
		if{
			0 (&gt;L:JPL_DOOR_COPILOT, bool)
		}	
	</Update>	

	<!--### WINDOWS SOUND HELPER ###-->
	<Update Frequency="5">
		(L:JPL_DOOR_PILOT) 1 &gt;=
		(L:JPL_WINDOW_PILOT, position 16k) 1 &gt;= or
		(L:JPL_DOOR_COPILOT) 1 &gt;= or
		(L:JPL_WINDOW_COPILOT, position 16k) 1 &gt;= or
		if{
			1 (&gt;L:JPL_CANOPY_OPEN, Bool)
		}	
		els{
			0 (&gt;L:JPL_CANOPY_OPEN, Bool)
		}
	</Update>

	<!-- ### COPILOT WEIGHT/FOR WEPT ### 
	<Update Frequency="2">
		(L:XMLVAR_AUDIO_SPKR_SEL, Bool) 1 ==
		if{
			170 (&gt;A:PAYLOAD STATION WEIGHT:2, pounds)
		}	
		(L:XMLVAR_AUDIO_SPKR_SEL, Bool) 0 ==
		if{
			0 (&gt;A:PAYLOAD STATION WEIGHT:2, pounds)
		}
	</Update>-->
	
	<!--### FUEL INBALANCE ###-->
	<Update Frequency="3">
		(A:TURN COORDINATOR BALL, position) 0.1 &gt;
		(A:FUEL LEFT QUANTITY, Gallons) 0.75 &gt; and
		if{ 2 (&gt;K:FUEL_SELECTOR_SET) }
		(A:TURN COORDINATOR BALL, position) -0.1 &lt;
		(A:FUEL RIGHT QUANTITY, Gallons) 0.75 &gt; and
		if{ 3 (&gt;K:FUEL_SELECTOR_SET) }
		(A:TURN COORDINATOR BALL, position) -0.1 &gt;= (A:TURN COORDINATOR BALL, position) 0.1 &lt;= and
		if{ 1 (&gt;K:FUEL_SELECTOR_SET) }
	</Update>	
	
<!-- ### CUSTOM RANDOMIZED MAG DROP EFFECT ### -->
	<UseTemplate Name="ASOBO_GT_Update">
		<UPDATE_ONCE>True</UPDATE_ONCE>
		<UPDATE_CODE>
			rand (&gt;O:JPL152_RANDOM_MAG)
		</UPDATE_CODE>	
	</UseTemplate>
	<Update Frequency="3">
		(A:RECIP ENG LEFT MAGNETO:1, bool) 0 ==
		if{
			(O:JPL152_RANDOM_MAG) .5 &gt; if{ (A:RECIP ENG MANIFOLD PRESSURE:1, psi) 1.9 - (&gt;A:RECIP ENG MANIFOLD PRESSURE:1, psi) } els{ (A:RECIP ENG MANIFOLD PRESSURE:1, psi) 1.8 - (&gt;A:RECIP ENG MANIFOLD PRESSURE:1, psi) }
		}
		(A:RECIP ENG RIGHT MAGNETO:1, bool) 0 ==
		if{
			(O:JPL152_RANDOM_MAG) .5 &gt; if{ (A:RECIP ENG MANIFOLD PRESSURE:1, psi) 2.4 - (&gt;A:RECIP ENG MANIFOLD PRESSURE:1, psi) } els{ (A:RECIP ENG MANIFOLD PRESSURE:1, psi) 2.3 - (&gt;A:RECIP ENG MANIFOLD PRESSURE:1, psi) }
		}
	</Update>
	
	<UseTemplate Name="ASOBO_GT_Update">
		<UPDATE_ONCE>True</UPDATE_ONCE>
		<UPDATE_CODE>
			0 (&gt;A:CIRCUIT SWITCH ON:42, bool)
			0 (&gt;A:CIRCUIT SWITCH ON:43, bool)
		</UPDATE_CODE>	
	</UseTemplate>	
	<Update Frequency="3">
		(A:PILOT TRANSMITTING, bool) 1 ==
		(A:COPILOT TRANSMITTING, bool) 1 == or
		(A:COM TRANSMIT:1, bool) 1 == and
		if{ 1 (&gt;A:CIRCUIT SWITCH ON:42, bool) }
		els{ 0 (&gt;A:CIRCUIT SWITCH ON:42, bool) }
	</Update>
	<Update Frequency="3">
		(A:PILOT TRANSMITTING, bool) 1 ==
		(A:COPILOT TRANSMITTING, bool) 1 == or
		(A:COM TRANSMIT:2, bool) 1 == and
		if{ 1 (&gt;A:CIRCUIT SWITCH ON:43, bool) }
		els{ 0 (&gt;A:CIRCUIT SWITCH ON:43, bool) }
	</Update>
</Template>

