<Template Name="JPL_Sun_Visors">
  <DefaultTemplateParameters>
    <USE_TRAJECTORY_DRAG_MODE>True</USE_TRAJECTORY_DRAG_MODE>
    <SCROLL_INCREMENT>655.36</SCROLL_INCREMENT>
  </DefaultTemplateParameters>
  <Component ID="#NODE_ID#" Node="#NODE_ID#">
    <UseTemplate Name="ASOBO_GT_Lever_DraggingWheelLeftDrag_Axis">
      <CURSOR>Grab</CURSOR>
      <NO_ARROWS/>
      <DRAG_CODE>
        16384 (M:DragPercent) *
        16384 min 0 max (&gt;L:JPL152_VISOR_#ID#)
      </DRAG_CODE>
      <ANIM_CODE>(L:JPL152_VISOR_#ID#, position 16k) 16384 / 100 *</ANIM_CODE>
      <ANIM_NAME>JPL_VISOR_#ID#</ANIM_NAME>
      <DRAG_ANIM_NAME>JPL_VISOR_#ID#</DRAG_ANIM_NAME>
      <POSITIVE_AXIS_CODE>(L:JPL152_VISOR_#ID#, position 16k) #SCROLL_INCREMENT# + 16384 min (&gt;L:JPL152_VISOR_#ID#)</POSITIVE_AXIS_CODE>
      <NEGATIVE_AXIS_CODE>(L:JPL152_VISOR_#ID#, position 16k) #SCROLL_INCREMENT# - 0 max (&gt;L:JPL152_VISOR_#ID#)</NEGATIVE_AXIS_CODE>
      <TOOLTIPID>#ID#'s Sun Visor</TOOLTIPID>
      <WWISE_EVENT_1>cabin_heat_lever_on</WWISE_EVENT_1>
      <WWISE_EVENT_2>audio_switch_intercom</WWISE_EVENT_2> 
    </UseTemplate>
  </Component>
</Template> 

<Template Name="JPL_Seats">
	<DefaultTemplateParameters>
	  <USE_TRAJECTORY_DRAG_MODE>True</USE_TRAJECTORY_DRAG_MODE>
	  <SCROLL_INCREMENT>655.36</SCROLL_INCREMENT>
	</DefaultTemplateParameters>
	<Component ID="#NODE_ID#" Node="#NODE_ID#">
	  <UseTemplate Name="ASOBO_GT_Lever_DraggingWheelLeftDrag_Axis">
		<CURSOR>Grab</CURSOR>
		<NO_ARROWS/>
		<DRAG_CODE>
		  (L:C152_#ID#sState) 0 == if{
		    16384 (M:DragPercent) *
		    16384 min 0 max (&gt;L:JPL152_#ID#_Seat)
		  }
		  (L:C152_#ID#sState) 1 == if{
		    16384 (M:DragPercent) *
		    5950 min 0 max (&gt;L:JPL152_#ID#_Seat)
		  }
		</DRAG_CODE>
		<ANIM_CODE>(L:JPL152_#ID#_Seat, position 16k) 16384 / 100 *</ANIM_CODE>
		<ANIM_NAME>#ID#SeatAnim</ANIM_NAME>
		<DRAG_ANIM_NAME>#ID#SeatAnim</DRAG_ANIM_NAME>
		<POSITIVE_AXIS_CODE>
		  (L:C152_#ID#sState) 0 == if{
			(L:JPL152_#ID#_Seat, position 16k) #SCROLL_INCREMENT# + 16384 min (&gt;L:JPL152_#ID#_Seat)
		  }
		  (L:C152_#ID#sState) 1 == if{
			(L:JPL152_#ID#_Seat, position 16k) #SCROLL_INCREMENT# + 5950 min (&gt;L:JPL152_#ID#_Seat)
		  }
		</POSITIVE_AXIS_CODE>
		<NEGATIVE_AXIS_CODE>(L:JPL152_#ID#_Seat, position 16k) #SCROLL_INCREMENT# - 0 max (&gt;L:JPL152_#ID#_Seat)</NEGATIVE_AXIS_CODE>
		<TOOLTIPID>#ID#'s Seat</TOOLTIPID>
		<WWISE_EVENT_1>cabin_heat_lever_on</WWISE_EVENT_1>
		<WWISE_EVENT_2>audio_switch_intercom</WWISE_EVENT_2> 
	  </UseTemplate>
	  <Update Frequency="8">
		(L:C152_#ID#sState) 1 ==
		(L:JPL152_#ID#_Seat, position 16k) 5950 &gt; and
		if{ 5950 (&gt;L:JPL152_#ID#_Seat, position 16k) }
	  </Update>
	</Component>
</Template> 

<Template Name="JPL_Window">
  <DefaultTemplateParameters>
    <USE_TRAJECTORY_DRAG_MODE>True</USE_TRAJECTORY_DRAG_MODE>
    <SCROLL_INCREMENT>1638.5</SCROLL_INCREMENT>
  </DefaultTemplateParameters>
  <Component ID="#NODE_ID#" Node="#NODE_ID#">
    <UseTemplate Name="ASOBO_GT_Lever_DraggingWheelLeftDrag_Axis">
      <CURSOR>Grab</CURSOR>
      <NO_ARROWS/>
      <DRAG_CODE>
	    (L:JPL_WINDOW_HANDLE_#ID#) 1 == if{
			16384 (M:DragPercent) *
			16384 min 0 max (&gt;L:JPL_WINDOW_#ID#, position 16k)
		}
	  
		(L:JPL_WINDOW_HANDLE_#ID#) 0 == 
		(L:JPL_WINDOW_#ID#, position 16k) 3158 &gt; and if{
			16384 (M:DragPercent) *
			16384 min 3159 max (&gt;L:JPL_WINDOW_#ID#, position 16k)
		}
      </DRAG_CODE>
      <ANIM_CODE>(L:JPL_WINDOW_#ID#, position 16k) 16384 / 100 *</ANIM_CODE>
      <ANIM_NAME>#ID#_Window</ANIM_NAME>
      <DRAG_ANIM_NAME>#ID#_Window</DRAG_ANIM_NAME>
      <POSITIVE_AXIS_CODE>
		(L:JPL_WINDOW_HANDLE_#ID#) 1 == if{
			(L:JPL_WINDOW_#ID#, position 16k) #SCROLL_INCREMENT# + 16384 min (&gt;L:JPL_WINDOW_#ID#, position 16k)
		}
		
		(L:JPL_WINDOW_HANDLE_#ID#) 0 == 
		(L:JPL_WINDOW_#ID#, position 16k) 0 &gt; and if{
			(L:JPL_WINDOW_#ID#, position 16k) #SCROLL_INCREMENT# + 16384 min (&gt;L:JPL_WINDOW_#ID#, position 16k)
		}
	  </POSITIVE_AXIS_CODE>
	  
      <NEGATIVE_AXIS_CODE>
	    (L:JPL_WINDOW_HANDLE_#ID#) 1 == if{
			(L:JPL_WINDOW_#ID#, position 16k) #SCROLL_INCREMENT# - 0 max (&gt;L:JPL_WINDOW_#ID#, position 16k)
		}
		
	    (L:JPL_WINDOW_HANDLE_#ID#) 0 == 
		(L:JPL_WINDOW_#ID#, position 16k) 0 &gt; and if{
			(L:JPL_WINDOW_#ID#, position 16k) #SCROLL_INCREMENT# - 3159 max (&gt;L:JPL_WINDOW_#ID#, position 16k)
		}
	  </NEGATIVE_AXIS_CODE>
    </UseTemplate>
  </Component>
</Template>

