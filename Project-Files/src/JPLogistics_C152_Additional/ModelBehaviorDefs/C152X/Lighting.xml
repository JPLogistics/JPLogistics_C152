<Template Name = "JPL152_Annun_Test">
	<UseTemplate Name = "JPL152_pushbutton_xml">
		<TOOLTIP_TITLE_IS_DYNAMIC>True</TOOLTIP_TITLE_IS_DYNAMIC>
		<TOOLTIP_TITLE>'TEST ANNUNCIATORS'</TOOLTIP_TITLE>
		<WWISE_EVENT_1>navcom_audio_push_on</WWISE_EVENT_1>
		<WWISE_EVENT_2>navcom_audio_push_off</WWISE_EVENT_2>
		<LEFT_SINGLE_CODE>1 (&gt;K:MARKER_BEACON_TEST_MUTE)</LEFT_SINGLE_CODE>
		<LEFT_LEAVE_CODE>0 (&gt;K:MARKER_BEACON_TEST_MUTE)</LEFT_LEAVE_CODE>
		<ANIM_CODE>(A:MARKER BEACON TEST MUTE, Bool) 100 *</ANIM_CODE>	
		<SWITCH_POSITION_TYPE>L</SWITCH_POSITION_TYPE>
		<SWITCH_POSITION_VAR>JPL_MARKER_TEST</SWITCH_POSITION_VAR>
	</UseTemplate>		
	<Update Frequency = "5">
	(L:JPL_MARKER_TEST, bool) 1 ==
	if{
		1 (&gt;K:MARKER_BEACON_TEST_MUTE, Bool)
	}
	(L:JPL_MARKER_TEST, bool) 0 ==	
	if{
		0 (&gt;K:MARKER_BEACON_TEST_MUTE, Bool)
	}
	</Update>
</Template>

<!-- ########### DO NOT DELETE, THINK OF AS A SUBTEMPLATE ########## -->
<Template Name = "JPL152_pushbutton_xml">
	<DefaultTemplateParameters>
		<SWITCH_POSITION_TYPE>L</SWITCH_POSITION_TYPE>
		<SWITCH_POSITION_VAR>buttonposition</SWITCH_POSITION_VAR>
		<LEFT_SINGLE_CODE></LEFT_SINGLE_CODE>
		<LEFT_LEAVE_CODE></LEFT_LEAVE_CODE>
		<INIT_VALUE>0</INIT_VALUE>
		<ANIMREF_ID>0</ANIMREF_ID>
		<ANIMTIP_0_ON_PERCENT>0</ANIMTIP_0_ON_PERCENT>
		<ANIMTIP_1_ON_PERCENT>1</ANIMTIP_1_ON_PERCENT>
	</DefaultTemplateParameters>
	<Component ID="#NODE_ID#" Node="#NODE_ID#">
		<Update Once="True">
			#INIT_VALUE# (&gt;#SWITCH_POSITION_TYPE#:#SWITCH_POSITION_VAR#)
		</Update>
		<UseTemplate Name = "ASOBO_GT_Anim_Code">
			<ANIM_CODE>(#SWITCH_POSITION_TYPE#:#SWITCH_POSITION_VAR#) 100 *</ANIM_CODE>
		</UseTemplate>
		<UseTemplate Name = "ASOBO_GT_AnimTriggers_2SoundEvents">
		</UseTemplate>
		<UseTemplate Name = "ASOBO_GT_Interaction_LeftSingle_Leave_Code">
			<LEFT_SINGLE_CODE>
				(#SWITCH_POSITION_TYPE#:#SWITCH_POSITION_VAR#) ! (&gt;#SWITCH_POSITION_TYPE#:#SWITCH_POSITION_VAR#)
			</LEFT_SINGLE_CODE>
			<LEFT_LEAVE_CODE>
				(#SWITCH_POSITION_TYPE#:#SWITCH_POSITION_VAR#) ! (&gt;#SWITCH_POSITION_TYPE#:#SWITCH_POSITION_VAR#)
			</LEFT_LEAVE_CODE>
		</UseTemplate>
	</Component>
</Template>

<Template Name = "JPL152_Light_Potentiometers">
	<DefaultTemplateParameters>
		<ANIM_NAME></ANIM_NAME>
		<NODE_ID></NODE_ID>
		<KNOB_POSITION_VAR_TYPE>L</KNOB_POSITION_VAR_TYPE>
		<KNOB_POSITION_VAR>knobposition</KNOB_POSITION_VAR>
		<KNOB_SPEED>5</KNOB_SPEED>
		<COUNT>20</COUNT>
		<MINIMUM>5</MINIMUM>
		<MAXIMUM>100</MAXIMUM>
		<ANIM_LAG>200</ANIM_LAG>
	</DefaultTemplateParameters>
	
	<Component ID="#NODE_ID#" Node="#NODE_ID#">
		<Condition Check="INIT_VALUE">
			<Update Once="True">
				#INIT_VALUE# (&gt;#KNOB_POSITION_VAR_TYPE#:#KNOB_POSITION_VAR#)
			</Update>
		</Condition>
		<Condition Check="LIGHT_TYPE">
			<Update>
				(#SIMVAR#) 0 &gt;
				if{ 1 (&gt;K:#LIGHT_TYPE#_LIGHTS_ON) }
				(#SIMVAR#) 0 ==
				if{ 1 (&gt;K:#LIGHT_TYPE#_LIGHTS_OFF) }
			</Update>
		</Condition>		
		<UseTemplate Name="ASOBO_GT_Knob_Finite_Code">
			<PART_ID>#PART_ID#</PART_ID>
			<ANIM_LENGTH>100</ANIM_LENGTH>
			<ANIM_CODE>
				(#KNOB_POSITION_VAR_TYPE#:#KNOB_POSITION_VAR#)
			</ANIM_CODE>
			<CLOCKWISE_CODE>
				(#KNOB_POSITION_VAR_TYPE#:#KNOB_POSITION_VAR#) #KNOB_SPEED# + #MAXIMUM# min (&gt;#KNOB_POSITION_VAR_TYPE#:#KNOB_POSITION_VAR#)
				(#KNOB_POSITION_VAR_TYPE#:#KNOB_POSITION_VAR#) (&gt;K:LIGHT_POTENTIOMETER_#POTENTIOMETER#_SET)
			</CLOCKWISE_CODE>
			<ANTICLOCKWISE_CODE>
				(#KNOB_POSITION_VAR_TYPE#:#KNOB_POSITION_VAR#) #KNOB_SPEED# - #MINIMUM# max (&gt;#KNOB_POSITION_VAR_TYPE#:#KNOB_POSITION_VAR#)
				(#KNOB_POSITION_VAR_TYPE#:#KNOB_POSITION_VAR#) (&gt;K:LIGHT_POTENTIOMETER_#POTENTIOMETER#_SET)
			</ANTICLOCKWISE_CODE>
		</UseTemplate>	
		<Condition Check="WWISE_EVENT">
			<UseTemplate Name = "ASOBO_GT_AnimTriggers_SoundEvents_Same"/>			
		</Condition>
	</Component>
	<Update>
		(#KNOB_POSITION_VAR_TYPE#:#KNOB_POSITION_VAR#) (&gt;K:LIGHT_POTENTIOMETER_#POTENTIOMETER#_SET)
	</Update>
</Template>

<Template Name = "JPL152_BATTERY_CHARGE">
	<UseTemplate Name = "JPL152_2way_switch_xml">
		<WWISE_EVENT_1>navcom_audio_push_on</WWISE_EVENT_1>
		<WWISE_EVENT_2>navcom_audio_push_off</WWISE_EVENT_2>
		<TOOLTIP_TITLE_IS_DYNAMIC>True</TOOLTIP_TITLE_IS_DYNAMIC>
		<TOOLTIP_TITLE>(A:ELECTRICAL BATTERY VOLTAGE, volts) 'Battery (%1.1f)' (F:Format)</TOOLTIP_TITLE>
		<SWITCH_POSITION_TYPE>L</SWITCH_POSITION_TYPE>
		<SWITCH_POSITION_VAR>JPL152_BATTERY_CHARGE</SWITCH_POSITION_VAR>
		<LEFT_SINGLE_CODE>
			(L:JPL152_BATTERY_CHARGE)
			(A:EXTERNAL POWER ON, bool) ! and 
			if{
				0 (&gt;K:TOGGLE_EXTERNAL_POWER)
			}
			(L:JPL152_BATTERY_CHARGE) !
			(A:EXTERNAL POWER ON, bool) and 
			if{
				0 (&gt;K:TOGGLE_EXTERNAL_POWER)
			}
		</LEFT_SINGLE_CODE>
	</UseTemplate>
	<Update>
		(L:JPL152_BATTERY_CHARGE) 1 ==
		(A:ELECTRICAL BATTERY VOLTAGE, volts) 23.90 &gt;= and
		(A:EXTERNAL POWER ON, bool) and
		if{
			0 (&gt;L:JPL152_BATTERY_CHARGE)
			0 (&gt;K:TOGGLE_EXTERNAL_POWER)
		}
	</Update>
</Template>

